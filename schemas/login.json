{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://randomrhythms.com/login.schema.json",
    "type": "object",
    "properties": {
      "email": {
        "type": "string",
        "format": "email"
      },
      "password": {
        "type": "string",
        "minLength": 8,
        "maxLength": 50
      }
    },
    "additionalProperties": false,
    "required": ["email", "password"]
  } 